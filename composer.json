{
    "name": "mr-vaco/menu-manager",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "mr-vaco/nova-panels": "^1.0.5",
        "mr-vaco/nova-statuses-manager": "^1.0",
        "mr-vaco/some-helper-code": "^0.1",
        "outl1ne/nova-sortable": "^3.4",
        "spatie/eloquent-sortable": "^4.0"
    },
    "require-dev": {
        "laravel-lang/common": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "MrVaco\\MenuManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrVaco\\MenuManager\\MenuServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
